# -----------------------------------------------------------------------------
# Pipeline Settings
# -----------------------------------------------------------------------------
pipeline:
  frequency: daily
  backfill_years: 5
  company_limit: 20

# -----------------------------------------------------------------------------
# Logging
# -----------------------------------------------------------------------------
logging:
  run_log_path: logs/pipeline_runs.jsonl

# -----------------------------------------------------------------------------
# Alpha Vantage API
# -----------------------------------------------------------------------------
api:
  alpha_vantage_key: "YOUR_API_KEY_HERE"

# -----------------------------------------------------------------------------
# Companies to extract (add or remove tickers freely)
# -----------------------------------------------------------------------------
companies:
  - AAPL    # Apple
  - MSFT    # Microsoft
  - GOOGL   # Alphabet (Google)
  - AMZN    # Amazon
  - JPM     # JPMorgan Chase

# -----------------------------------------------------------------------------
# MinIO Object Storage
# Default credentials from docker-compose.yml
# -----------------------------------------------------------------------------
minio:
  endpoint: "localhost:9000"
  access_key: "minioadmin"
  secret_key: "minioadmin"
  bucket: "data-lake"
  secure: false

# -----------------------------------------------------------------------------
# PostgreSQL Database
# Default credentials from docker-compose.yml
# -----------------------------------------------------------------------------
database:
  host: localhost
  port: 5432
  name: fift
  schema: systematic_equity
  user: postgres
  password: postgres